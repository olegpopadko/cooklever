{% extends '::base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="fos">
            <div class="row">
                {% for type, messages in app.session.flashBag.all %}
                    {% for message in messages %}
                        <div class="{{ type }}">
                            {{ message|trans({}, 'FOSUserBundle') }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
            {% block fos_user_content %}{% endblock %}
        </div>
    </div>
{% endblock %}

